<Window x:Class="Lopez_PrelimProj_IntegAndEventDriven.Leaderboard"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Lopez_PrelimProj_IntegAndEventDriven"
        mc:Ignorable="d"
        Title="Leaderboard" Height="300" Width="400">
    <Grid>
        <Label x:Name="labelNum" Content="Leaderboard" HorizontalAlignment="Left" Margin="10,10,0,0" VerticalAlignment="Top" FontSize="18" FontFamily="Copperplate Gothic Bold"/>
        <ListBox x:Name="listboxNames" HorizontalAlignment="Center" Height="208" Width="380" HorizontalContentAlignment="Center" VerticalAlignment="Top" Margin="0,45,0,0" FontFamily="Arial"/>
        <ComboBox x:Name="comboboxDifficulty" HorizontalAlignment="Left" Margin="161,14,0,0" VerticalAlignment="Top" Width="120" SelectionChanged="comboboxDifficulty_SelectionChanged" FontFamily="Arial"/>

    </Grid>
</Window>
